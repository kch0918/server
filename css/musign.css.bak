/* login 페이지 */

.join-view .text-center {
	margin-bottom:70px !important;
}

.join-view .log-fot .log-l a:hover:after {
	content:"";
	display:block;
	width:100%;
	height:1px;
	background:#333;
}

.join-view .log-fot .log-r a:hover:after {
	content:"";
	display:block;
	width:100%;
	height:1px;
	background:#333;
}

.join-view .login-wr > div:nth-child(3) {
