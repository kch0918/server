/* Modify */

.modify .join-box .join-intit {
	color:#00a6b6 !important;
}

.login-wr .join-box div input {
	padding: 10px 0;
}

.modify_btn .del-btn {
	font-size: 20px;
    color: #fff;
    display: inline-block;
	width:230px;
    height: 90px;
    line-height: 90px;
    background-color: #adadad;
    border: 1px solid #adadad;
    text-align: center;
    cursor: pointer;
}

.modify_btn .login-btn {
	font-size: 20px;
    color: #fff;
    display: inline-block;
	width:230px;
    height: 90px;
    line-height: 90px;
    background-color: #00a6b6;
    border: 1px solid #00a6b6;
    text-align: center;
    cursor: pointer;
	margin-left: 16px;
}

.ser_info {
	margin-top:20px;
}

.ser_info li {
	font-size:16px;
	color:#666;
	margin:45px 0;
}

.ser_info li:last-child {
	margin-bottom:100px;
}

.ser_info li:nth-child(3) span , .ser_info li:nth-child(4) span {
	margin: 0 0 0 107px;
}

.ser_info li:nth-child(5) span {
	margin: 0 0 0 65px;
}

.ser_info li:nth-child(6) span {
	margin: 0 0 0 70px;
}

.ser_info li span {
	font-weight:300;
	margin:0 0 0 77px;
}


/* PRICE MANAGEMENT */

#fncForm .text-center {
	margin-bottom:100px;
}


.price_manage {
	max-width: 1213px;
    margin: 0 auto;
    padding: 100px 30px 0;
}

#fncForm > div > input {
    width: 1160px;
    padding: 88px 0;
    background: #fff;
	border-radius:15px;
	margin-bottom:30px;
	font-size:40px;
	color:#00a6b6;
	font-weight:600;
	border-bottom:none;
	box-shadow: 5.5px 9.5px 20px 5px rgba(61, 61, 61, 0.19);
}

#fncForm > div > input:focus {
	border: 5px solid #00a6b6;
	background:url(/img/icon-check.png) no-repeat -15px center;
}

#fncForm > div > input:nth-child(6) {
	display:none;
}

#fncForm > div > input:nth-child(7) {
	display:none;
}

#fncForm > div > input:nth-child(8) {
	display:none;
}

#server_info {
	display:none;
}

.price_btn {
	padding:80px 0 0;
	text-align:center;
}

.price_btn .del-btn {
	font-size: 20px;
    color: #fff;
    display: inline-block;
	width:230px;
    height: 90px;
    line-height: 90px;
    background-color: #adadad;
    border: 1px solid #adadad;
    text-align: center;
    cursor: pointer;
}

.price_btn .login-btn {
	font-size: 20px;
    color: #fff;
    display: inline-block;
	width:230px;
    height: 90px;
    line-height: 90px;
